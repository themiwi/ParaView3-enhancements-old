<ParaViewReaders>
  <!-- Order is important.  The readers with the most strict CanReadFile -->
  <!-- methods should appear first as they will be checked first and -->
  <!-- used first. -->
  <Reader name="netCDFReader"
          extensions="ncdf nc"
          file_description="netCDF Files">
  </Reader>
  <Reader name="SLACReader"
          extensions="ncdf nc"
          file_description="SLAC Files">
  </Reader>
  <Reader name="SLACParticleReader"
          extensions="ncdf netcdf"
          file_description="SLAC Particle Files">
  </Reader>
</ParaViewReaders>
