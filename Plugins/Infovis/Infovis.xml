    <SourceProxy name="GroupLeafVertices" class="vtkGroupLeafVertices">
      <InputProperty name="Input" command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkTree"/>
        </DataTypeDomain>
        <InputArrayDomain name="vertex_arrays" attribute_type="vertex"/>
      </InputProperty>

      <StringVectorProperty name="GroupArray" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" animateable="0"> 
        <ArrayListDomain name="array_list" input_domain_name="vertex_arrays">
          <RequiredProperties>
            <Property name="Input" function="Input"/>
          </RequiredProperties>
        </ArrayListDomain>
      </StringVectorProperty> 

      <StringVectorProperty name="NameArray" command="SetInputArrayToProcess" number_of_elements="5" element_types="0 0 0 0 2" default_values="1" animateable="0"> 
        <ArrayListDomain name="array_list" input_domain_name="vertex_arrays">
          <RequiredProperties>
            <Property name="Input" function="Input"/>
          </RequiredProperties>
        </ArrayListDomain>
      </StringVectorProperty> 

      <Hints>
        <View type="ClientGraphView"/>
      </Hints>
