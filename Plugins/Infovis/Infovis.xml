      <IntVectorProperty name="FieldType" command="SetFieldType" number_of_elements="1" default_values="0">
        <EnumerationDomain name="enum">
          <Entry value="0" text="Point Data"/>
          <Entry value="1" text="Cell Data"/>
          <Entry value="2" text="Vertex Data"/>
          <Entry value="3" text="Edge Data"/>
          <Entry value="4" text="Row Data"/>
        </EnumerationDomain>
      </IntVectorProperty>
      
      <IntVectorProperty name="OutputArrayType" command="SetOutputArrayType" number_of_elements="1" default_values="6">
      </IntVectorProperty>

      <IntVectorProperty name="PassArray" command="SetPassArray" number_of_elements="1" default_values="0">
        <BooleanDomain name="bool" />
      </IntVectorProperty>

      <DoubleVectorProperty name="FillValue" command="SetFillValue" number_of_elements="1" default_values="-1">
      </DoubleVectorProperty>

      <StringVectorProperty name="InputArrayName" command="SetInputArrayName" number_of_elements="1" default_values="">
      </StringVectorProperty>

      <StringVectorProperty name="OutputArrayName" command="SetOutputArrayName" number_of_elements="1" default_values="ArrayMap">
      </StringVectorProperty>
      
      <StringVectorProperty
        name="Map"
        command="AddToMap"
        repeat_command="1"
        number_of_elements_per_command="2"
        clean_command="ClearMap"
        animateable="0">
      </StringVectorProperty>
    </SourceProxy>

