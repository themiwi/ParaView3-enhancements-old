<ServerManagerConfiguration>

  <ProxyGroup name="layout_strategies">
    <Proxy name="Random" class="vtkRandomLayoutStrategy"/>
    <Proxy name="ForceDirected" class="vtkForceDirectedLayoutStrategy"/>
    <Proxy name="Simple2D" class="vtkSimple2DLayoutStrategy"/>
    <Proxy name="Clustering2D" class="vtkClustering2DLayoutStrategy"/>
    <Proxy name="Fast2D" class="vtkFast2DLayoutStrategy"/>
    <Proxy name="Circular" class="vtkCircularLayoutStrategy"/>
    <Proxy name="Tree" class="vtkTreeLayoutStrategy"/>
    <Proxy name="TreeOrbit" class="vtkTreeOrbitLayoutStrategy"/>
    <Proxy name="Box" class="vtkBoxLayoutStrategy"/>
    <Proxy name="SliceAndDice" class="vtkSliceAndDiceLayoutStrategy"/>
    <Proxy name="Squarify" class="vtkSquarifyLayoutStrategy"/>
    <Proxy name="StackedTree" class="vtkStackedTreeLayoutStrategy"/>
    <Proxy name="None" class="vtkPassThroughLayoutStrategy"/>
  </ProxyGroup>

  <ProxyGroup name="sources">

    <SourceProxy name="DelimitedTextReader" class="vtkDelimitedTextReader">
      <StringVectorProperty name="FileName" command="SetFileName" number_of_elements="1">
        <FileListDomain name="files"/>
      </StringVectorProperty>
      <StringVectorProperty name="FieldDelimiterCharacters" command="SetFieldDelimiterCharacters" number_of_elements="1" default_values=","/>
      <IntVectorProperty name="UseStringDelimiter" command="SetUseStringDelimiter" number_of_elements="1" default_values="1"/>
      <IntVectorProperty name="HaveHeaders" command="SetHaveHeaders" number_of_elements="1" default_values="1"/>
      <IntVectorProperty name="MergeConsecutiveDelimiters" command="SetMergeConsecutiveDelimiters" number_of_elements="1" default_values="0"/>
      <IntVectorProperty name="MaxRecords" command="SetMaxRecords" number_of_elements="1" default_values="0"/>
      <IntVectorProperty name="DetectNumericColumns" command="SetDetectNumericColumns" number_of_elements="1" default_values="1"/>
      <IntVectorProperty name="OutputPedigreeIds" command="SetOutputPedigreeIds" number_of_elements="1" label="Output Pedigree IDs" default_values="1">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <IntVectorProperty name="GeneratePedigreeIds" command="SetGeneratePedigreeIds" number_of_elements="1" label="Generate Pedigree IDs" default_values="1">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <StringVectorProperty name="PedigreeIdArrayName" command="SetPedigreeIdArrayName" number_of_elements="1" label="Pedigree ID Array Name"/> 
      <Hints>
        <View type="ClientTableView"/>
      </Hints>
    </SourceProxy>

    <SourceProxy name="RandomGraphSource" class="vtkRandomGraphSource">
      <IntVectorProperty name="NumberOfVertices" command="SetNumberOfVertices" number_of_elements="1" default_values="10"/>  
      <IntVectorProperty name="NumberOfEdges" command="SetNumberOfEdges" number_of_elements="1" default_values="10"/>  
      <DoubleVectorProperty name="EdgeProbability" command="SetEdgeProbability" number_of_elements="1" default_values="0.5"/>  
      <IntVectorProperty name="IncludeEdgeWeights" command="SetIncludeEdgeWeights" number_of_elements="1" default_values="0"> 
        <BooleanDomain name="bool"/>
      </IntVectorProperty> 
      <IntVectorProperty name="Directed" command="SetDirected" number_of_elements="1" default_values="0"> 
        <BooleanDomain name="bool"/>
      </IntVectorProperty> 
      <IntVectorProperty name="UseEdgeProbability" command="SetUseEdgeProbability" number_of_elements="1" default_values="0"> 
        <BooleanDomain name="bool"/>
      </IntVectorProperty> 
      <IntVectorProperty name="StartWithTree" command="SetStartWithTree" number_of_elements="1" default_values="0"> 
        <BooleanDomain name="bool"/>
      </IntVectorProperty> 
      <IntVectorProperty name="AllowSelfLoops" command="SetAllowSelfLoops" number_of_elements="1" default_values="0"> 
        <BooleanDomain name="bool"/>
      </IntVectorProperty> 
      <IntVectorProperty name="AllowParallelEdges" command="SetAllowParallelEdges" number_of_elements="1" default_values="0"> 
        <BooleanDomain name="bool"/>
      </IntVectorProperty> 
      <IntVectorProperty name="Seed" command="SetSeed" number_of_elements="1" default_values="1177"/> 
      <StringVectorProperty name="VertexPedigreeIdArrayName"
        label="Vertex Pedigree ID Array Name"
        command="SetVertexPedigreeIdArrayName"
        number_of_elements="1"
        default_values="vertex id"/> 
      <StringVectorProperty name="EdgePedigreeIdArrayName"
        label="Edge Pedigree ID Array Name"
        command="SetEdgePedigreeIdArrayName"
        number_of_elements="1"
        default_values="edge id"/> 
      <StringVectorProperty name="EdgeWeightArrayName"
        label="Edge Weight Array Name"
        command="SetEdgeWeightArrayName"
        number_of_elements="1"
        default_values="edge weight"/> 
      <Hints>
        <View type="ClientGraphView"/>
      </Hints>
    </SourceProxy>

    <SourceProxy name="GeoRandomGraphSource" class="vtkGeoRandomGraphSource"
                 base_proxygroup="sources"   base_proxyname="RandomGraphSource">
        <Hints>
            <View type="ClientGeoView"/>
        </Hints>
    </SourceProxy>

    <SourceProxy name="SQLDatabaseGraphSource" class="vtkSQLDatabaseGraphSource">
      <StringVectorProperty name="URL" command="SetURL" number_of_elements="1" label="Database"/> 
      <StringVectorProperty name="Password" command="SetPassword" number_of_elements="1" label="Password" is_internal="1"/> 
      <StringVectorProperty name="EdgeQuery" command="SetEdgeQuery" number_of_elements="1">
        <Hints>
          <Widget type="multi_line"/>
        </Hints>
      </StringVectorProperty>
      <StringVectorProperty name="VertexQuery" command="SetVertexQuery" number_of_elements="1">
        <Hints>
          <Widget type="multi_line"/>
        </Hints>
      </StringVectorProperty>
      <StringVectorProperty name="LinkVertices" command="AddLinkVertex" clean_command="ClearLinkVertices" repeat_command="1" number_of_elements_per_command="3" element_types="2 2 0"/>
      <StringVectorProperty name="LinkEdges" command="AddLinkEdge" clean_command="ClearLinkEdges" repeat_command="1" number_of_elements_per_command="2" element_types="2 2"/>

      <IntVectorProperty name="Directed" command="SetDirected" number_of_elements="1" default_values="1"> 
        <BooleanDomain name="bool"/>
      </IntVectorProperty> 

      <IntVectorProperty name="GenerateEdgePedigreeIds" command="SetGenerateEdgePedigreeIds" number_of_elements="1" label="Generate Edge Pedigree IDs" default_values="1">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <StringVectorProperty name="EdgePedigreeIdArrayName" command="SetEdgePedigreeIdArrayName" number_of_elements="1" label="Edge Pedigree ID Array Name"/> 
      <Hints>
        <View type="ClientGraphView"/>
      </Hints>
    </SourceProxy>

    <SourceProxy name="SQLDatabaseTableSource" class="vtkSQLDatabaseTableSource">
      <StringVectorProperty name="URL" command="SetURL" number_of_elements="1" label="Database"/> 
      <StringVectorProperty name="Password" command="SetPassword" number_of_elements="1" label="Password" is_internal="1"/> 
      <StringVectorProperty name="Query" command="SetQuery" number_of_elements="1">
        <Hints>
          <Widget type="multi_line"/>
        </Hints>
      </StringVectorProperty>
      <IntVectorProperty name="GeneratePedigreeIds" command="SetGeneratePedigreeIds" number_of_elements="1" label="Generate Pedigree IDs" default_values="1">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <StringVectorProperty name="PedigreeIdArrayName" command="SetPedigreeIdArrayName" number_of_elements="1" label="Pedigree ID Array Name"/> 
      <Hints>
        <View type="ClientTableView"/>
      </Hints>
    </SourceProxy>

    <SourceProxy name="TulipReader" class="vtkTulipReader">
      <StringVectorProperty name="FileName" command="SetFileName" number_of_elements="1">
        <FileListDomain name="files"/>
      </StringVectorProperty>
      <Hints>
        <View type="ClientGraphView"/>
      </Hints>
    </SourceProxy>

    <SourceProxy name="XMLTreeReader" class="vtkXMLTreeReader">
      <StringVectorProperty name="FileName" command="SetFileName" number_of_elements="1">
        <FileListDomain name="files"/>
      </StringVectorProperty>
      <IntVectorProperty name="MaskArrays" command="SetMaskArrays" number_of_elements="1" default_values="0"> 
        <BooleanDomain name="bool"/>
      </IntVectorProperty> 
      <IntVectorProperty name="ReadCharData" command="SetReadCharData" number_of_elements="1" default_values="0"> 
        <BooleanDomain name="bool"/>
      </IntVectorProperty> 
      <IntVectorProperty name="Generate Vertex Pedigree Ids" command="SetGenerateVertexPedigreeIds" number_of_elements="1" default_values="1">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <StringVectorProperty name="Vertex Pedigree Id Array Name" command="SetVertexPedigreeIdArrayName" number_of_elements="1"/>
      <IntVectorProperty name="Generate Edge Pedigree Ids" command="SetGenerateEdgePedigreeIds" number_of_elements="1" default_values="1">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <StringVectorProperty name="Edge Pedigree Id Array Name" command="SetEdgePedigreeIdArrayName" number_of_elements="1"/>
      <Hints>
        <View type="ClientGraphView"/>
      </Hints>
    </SourceProxy>

   <SourceProxy name="TextSource" class="vtkPVTextSource" 
      label="Text">
     <Documentation
       short_help="Display text"
       long_help="The Text source generates a table containing text." >
       The Text source is used to generate a 1x1 vtkTable with a single text string.
     </Documentation>
     <StringVectorProperty
       name="Text"
       command="SetText"
       number_of_elements="1"
       default_values="Text">
             <Hints>
              <Widget type="multi_line"/>
            </Hints>
        <Documentation>
          This property specifies the text to display.
        </Documentation>
     </StringVectorProperty>
     <Hints>
       <View type="ClientGraphView"/>
     </Hints>
   </SourceProxy>

  </ProxyGroup>
  
  <!-- FILTERS GROUP -->
  <ProxyGroup name="filters">
    <SourceProxy name="SparseArrayToTable" class="vtkSparseArrayToTable">
      <InputProperty name="Input" command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkArrayData"/>
        </DataTypeDomain>
      </InputProperty>

      <StringVectorProperty name="ValueColumn" command="SetValueColumn" number_of_elements="1" default_values="index" label="Value Column"/>

    </SourceProxy>

    <SourceProxy name="MergeTables" class="vtkMergeTables">
      <!-- Table 1 Input -->
      <InputProperty name="Input1" command="SetInputConnection" port_index="0">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkTable"/>
        </DataTypeDomain>
      </InputProperty>
      <!-- Table 2 Input -->
      <InputProperty name="Input2" command="SetInputConnection" port_index="1">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkTable"/>
        </DataTypeDomain>
      </InputProperty>
      <IntVectorProperty 
        name="MergeColumnsByName" 
        command="SetMergeColumnsByName" 
        number_of_elements="1" 
        default_values="1">
        <BooleanDomain name="bool"/>
      </IntVectorProperty>
      <Hints>
        <View type="ClientTableView"/>
      </Hints>
    </SourceProxy>

