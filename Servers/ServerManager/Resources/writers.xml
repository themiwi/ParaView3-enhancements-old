<ServerManagerConfiguration>
  <ProxyGroup name="writers">
    <PVDWriterProxy name="XMLPVDWriter" class="vtkXMLPVDWriter">
      <InputProperty name="Input" command="SetInput">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkDataSet"/>
        </DataTypeDomain>
      </InputProperty>

      <StringVectorProperty name="FileName" command="SetFileName"/>

      <IntVectorProperty name="GhostLevel" command="SetGhostLevel">
        <IntRangeDomain name="range" min="0"/>
      </IntVectorProperty>

      <IntVectorProperty name="DataMode" command="SetDataMode">
        <EnumerationDomain name="enum">
          <Entry value="0" text="Ascii" />
          <Entry value="1" text="Binary" />
          <Entry value="2" text="Appended" />
        </EnumerationDomain>
      </IntVectorProperty>
    </PVDWriterProxy>

    <WriterProxy name="PLYWriter" class="vtkPLYWriter">
      <InputProperty name="Input" command="SetInput">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkPolyData"/>
        </DataTypeDomain>
      </InputProperty>

      <StringVectorProperty name="FileName" command="SetFileName"/>
    </WriterProxy>

    <WriterProxy name="MetaImageWriter" class="vtkMetaImageWriter">
      <InputProperty name="Input" command="SetInput">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkImageData"/>
        </DataTypeDomain>
      </InputProperty>

      <StringVectorProperty name="FileName" command="SetFileName"/>
    </WriterProxy>

    <WriterProxy name="PNGWriter" class="vtkPNGWriter">
      <InputProperty name="Input" command="SetInput">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkImageData"/>
        </DataTypeDomain>
      </InputProperty>

      <StringVectorProperty name="FileName" command="SetFileName"/>
    </WriterProxy>

    <WriterProxy name="XdmfWriter" class="vtkXdmfWriter">
      <InputProperty name="Input" command="SetInput">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkDataSet"/>
        </DataTypeDomain>
      </InputProperty>

      <StringVectorProperty name="FileName" command="SetFileName"/>
    </WriterProxy>

    <WriterProxy name="ExodusIIWriter" class="vtkExodusIIWriter">
      <InputProperty name="Input" command="SetInput">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkUnstructuredGrid"/>
        </DataTypeDomain>
      </InputProperty>

      <StringVectorProperty name="FileName" command="SetFileName"/>

      <IntVectorProperty name="GhostLevel" command="SetGhostLevel">
        <IntRangeDomain name="range" min="0"/>
      </IntVectorProperty>
    </WriterProxy>

    <WriterProxy name="EnSightWriter" class="vtkEnSightWriter">
      <InputProperty name="Input" command="SetInput">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkUnstructuredGrid"/>
        </DataTypeDomain>
      </InputProperty>

      <StringVectorProperty name="FileName" command="SetFileName"/>

      <IntVectorProperty name="GhostLevel" command="SetGhostLevel">
        <IntRangeDomain name="range" min="0"/>
      </IntVectorProperty>
    </WriterProxy>

    <XMLPVAnimationWriterProxy name="XMLPVAnimationWriter" class ="vtkXMLPVAnimationWriter">
      <StringVectorProperty name="FileName" command="SetFileName" />
      
      <IntVectorProperty 
        name="NumberOfPieces" 
        command="SetNumberOfPieces"
        number_of_elements="1"
        default_values="1">
        <IntRangeDomain name="range" min="-1"/>
      </IntVectorProperty>
<!--     
      <IntVectorProperty name="PieceInfo"
        command="GetPartitionId"
        number_of_elements="1"
        information_only="1">
        <SimpleIntInformationHelper/>
      </IntVectorProperty>

      <IntVectorProperty name="Piece" command="SetPiece"
        information_property="PieceInfo">
        <IntRangeDomain name="range" min="0" />
      </IntVectorProperty>
-->  
      
      <ProxyProperty 
        name="Input" 
        commad="AddInput"
        update_self="1">
        <!--What is the group for vtkSMPartDisplayProxy ? -->
      </ProxyProperty>
     
      <Property
        name="Start"
        command="Start"
        update_self="1"
        immediate_update="1">
      </Property>
      
      <DoubleVectorProperty 
        name="WriteTime" 
        command="WriteTime" 
        number_of_elements="1"
        update_self="1"
        default_values="0.0">
        <DoubleRangeDomain name="range" min="0" />
      </DoubleVectorProperty>
      
      <Property
        name="Finish"
        command="Finish"
        update_self="1"
        immediate_update="1">
      </Property>

    </XMLPVAnimationWriterProxy>

    <SummaryHelperProxy name="SummaryHelper" class="vtkPVSummaryHelper">
      <ProxyProperty name="Writer" command="SetWriter">
        <ProxyGroupDomain name="group">
          <Group name="writers" />
        </ProxyGroupDomain>
      </ProxyProperty>

      <Property
        name="SynchronizeSummaryFiles"
        command="SynchronizeSummaryFiles"
        immediate_update="1">
      </Property>
    </SummaryHelperProxy>

  </ProxyGroup>
</ServerManagerConfiguration>
