<ServerManagerConfiguration>
  <ProxyGroup name="writers">
    <WriterProxy name="XMLPVDWriter" class="vtkXMLPVDWriter">
      <InputProperty name="Input" command="SetInput">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkDataSet"/>
        </DataTypeDomain>
      </InputProperty>

      <StringVectorProperty name="FileName" command="SetFileName"/>

      <IntVectorProperty name="GhostLevel" command="SetGhostLevel">
        <IntRangeDomain name="range" min="0"/>
      </IntVectorProperty>

      <IntVectorProperty name="Piece" command="SetPiece">
        <IntRangeDomain name="range" min="0" max="1"/>
      </IntVectorProperty>

      <IntVectorProperty name="NumberOfPieces" command="SetNumberOfPieces">
        <IntRangeDomain name="range" min="-1"/>
      </IntVectorProperty>

      <IntVectorProperty name="WriteCollectionFile"
                         command="SetWriteCollectionFile" default_values="0">
        <IntRangeDomain name="range" min="0" max="1"/>
      </IntVectorProperty>
    </WriterProxy>

