<ServerManagerConfiguration>
<!-- TODO need input requirements -->
  <ProxyGroup name="filters">
   <SourceProxy name="AppendAttributes" class="vtkMergeArrays">
   <!-- End AppendAttributes -->
   </SourceProxy>

   <SourceProxy name="AppendPolyData" class="vtkAppendPolyData">
   <!-- End AppendPolyData -->
   </SourceProxy>

   <SourceProxy name="AppendDataSet" class="vtkAppendDataSet">
   <!-- End AppendDataSet -->
   </SourceProxy>

   <SourceProxy name="CellCenters" class="vtkCellCenters">
     <IntVectorProperty 
        name="GenerateVertices" 
        command="SetVertexCells" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain/>
     </IntVectorProperty>
   <!-- End CellCenters -->
   </SourceProxy>

   <SourceProxy name="CellDataToPointData" class="vtkPCellDataToPointData">
     <IntVectorProperty 
        name="PassCellData" 
        command="SetPassCellData" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain/>
     </IntVectorProperty>
   <!-- End CellDataToPointData -->
   </SourceProxy>

   <SourceProxy name="CleanPolyData" class="vtkCleanPolyData">
     <IntVectorProperty 
        name="PieceInvariant" 
        command="SetPieceInvariant" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain/>
     </IntVectorProperty>

      <DoubleVectorProperty
         name="Tolerance"
         command="SetTolerance"
         number_of_elements="1"
         default_values="0.0" >
        <DoubleRangeDomain min="0" max="1" />
      </DoubleVectorProperty>

      <DoubleVectorProperty
         name="AbsoluteTolerance"
         command="SetAbsoluteTolerance"
         number_of_elements="1"
         default_values="1.0" >
      </DoubleVectorProperty>

     <IntVectorProperty 
        name="ToleranceIsAbsolute" 
        command="SetToleranceIsAbsolute" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="ConvertLinesToPoints" 
        command="SetConvertLinesToPoints" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="ConvertPolysToLines" 
        command="SetConvertPolyToLines" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="ConvertStripsToPolys" 
        command="SetConvertStripsToPolys" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="PointMerging" 
        command="SetPointMerging" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain/>
     </IntVectorProperty>
   <!-- End CleanPolyData -->
   </SourceProxy>

   <SourceProxy name="CleanUnstructuredGrid" class="vtkCleanUnstructuredGrid">
   <!-- End CleanUnstructuredGrid -->
   </SourceProxy>

   <SourceProxy name="Connectivity" class="vtkPVConnectivityFilter">
   <!-- End Connectivity -->
   </SourceProxy>

   <SourceProxy name="Contour" class="vtkPVKitwareContourFilter">
     <StringVectorProperty 
        name="SelectInputScalars" 
        command="SelectInputScalars" 
        number_of_elements="1" /> 

     <DoubleVectorProperty
        name="ContourValues"
        command="SetValue"
        number_of_elements="1"
        default_values="0.0" 
        repeat_command="1"
        number_of_elements_per_command="1"
        use_index="1">
     </DoubleVectorProperty>

     <IntVectorProperty 
        name="ComputeNormals" 
        command="SetComputeNormals" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="ComputeGradients" 
        command="SetComputeGradients" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="ComputeScalars" 
        command="SetComputeScalars" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain/>
     </IntVectorProperty>
   <!-- End Contour -->
   </SourceProxy>

   <SourceProxy name="Calculator" class="vtkArrayCalculator">
     <StringVectorProperty 
        name="ResultArrayName" 
        command="SetResultArrayName" 
        number_of_elements="1"
        default_values="result" /> 

     <StringVectorProperty 
        name="Function" 
        command="SetFunction" 
        number_of_elements="1" /> 
   
     <StringVectorProperty 
        name="AddScalarVariable" 
        command="AddScalarVariable" 
        number_of_elements="3" 
        repeat_command="1"
        number_of_elements_per_command="3"
        element_types="2 2 0" /> 

     <StringVectorProperty 
        name="AddVectorVariable" 
        command="AddVectorVariable" 
        number_of_elements="5" 
        repeat_command="1"
        number_of_elements_per_command="5"
        element_types="2 2 0 0 0" /> 

     <IntVectorProperty 
        name="AttributeMode" 
        command="SetAttributeMode" 
        number_of_elements="1"
        default_values="0" >
       <EnumarationDomain 
          values="default:0 point_data:1 cell_data:2" />
     </IntVectorProperty>
   <!-- End Calculator -->
   </SourceProxy>


   <SourceProxy name="ExtractGrid" class="vtkPVExtractVOI">
     <IntVectorProperty 
        name="VOI" 
        command="SetVOI"
        number_of_elements="6"/>

     <IntVectorProperty 
        name="SampleRate" 
        command="SetSampleRate"
        number_of_elements="3"/>

     <IntVectorProperty 
        name="IncludeBoundary" 
        command="SetIncludeBoundary" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain/>
     </IntVectorProperty>
   <!-- End ExtractGrid -->
   </SourceProxy>

   <SourceProxy name="CTHPart" class="vtkExtractCTHPart">
     <ProxyProperty name="ClipPlane" command="SetClipPlane"/>

     <StringVectorProperty 
        name="AddVolumeArrayName" 
        command="AddVolumeArrayName" 
        repeat_command="1"
        number_of_elements_per_command="1"
        number_of_elements="1" /> 
   <!-- End CTHPart -->
   </SourceProxy>

   <SourceProxy name="Cut" class="vtkKitwareCutter">
     <ProxyProperty name="CutFunction" command="SetCutFunction"/>

     <DoubleVectorProperty
        name="ContourValues"
        command="SetValue"
        number_of_elements="1"
        default_values="0.0" 
        repeat_command="1"
        number_of_elements_per_command="1"
        use_index="1">
     </DoubleVectorProperty>
   <!-- End Cut -->
   </SourceProxy>

   <SourceProxy name="ExtractParts" class="vtkSelectInputs">
     <IntVectorProperty 
        name="InputMask" 
        command="SetInputMask"
        number_of_elements="1"
        repeat_command="1"
        number_of_elements_per_command="2" />
   <!-- End ExtractParts -->
   </SourceProxy>

   <SourceProxy name="Glyph" class="vtkPVGlyphFilter">
     <StringVectorProperty 
        name="SelectInputScalars" 
        command="SelectInputScalars" 
        number_of_elements="1" /> 

     <StringVectorProperty 
        name="SelectInputVectors" 
        command="SelectInputVectors" 
        number_of_elements="1" /> 

     <IntVectorProperty 
        name="SetOrient" 
        command="SetOrient" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="SetScaleMode" 
        command="SetScaleMode" 
        number_of_elements="1"
        default_values="0" >
       <EnumarationDomain 
          values="scalar:0 vector:1 vector_components:2 off:3" />
     </IntVectorProperty>

      <DoubleVectorProperty
         name="SetScaleFactor"
         command="SetScaleFactor"
         number_of_elements="1"
         default_values="1.0" >
      </DoubleVectorProperty>

     <IntVectorProperty 
        name="MaximumNumberOfPoints" 
        command="SetMaximumNumberOfPoints" 
        number_of_elements="1"
        default_values="5000" >
     </IntVectorProperty>

     <IntVectorProperty 
        name="UseMaskPoints" 
        command="SetUseMaskPoints" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain/>
     </IntVectorProperty>

     <IntVectorProperty 
        name="RandomMode" 
        command="SetRandomMode" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain/>
     </IntVectorProperty>
   <!-- End Glyph -->
   </SourceProxy>

   <SourceProxy name="GroupParts" class="vtkGroup">
   <!-- End GroupParts -->
   </SourceProxy>

   <SourceProxy name="Threshold" class="vtkPVThresholdFilter">
     <StringVectorProperty 
        name="SelectInputScalars" 
        command="SelectInputScalars" 
        number_of_elements="1" /> 

     <IntVectorProperty 
        name="AttributeMode" 
        command="SetAttributeMode"
        number_of_elements="1">
       <EnumarationDomain 
          values="point_data:1 cell_data:2" />
     </IntVectorProperty>

     <DoubleVectorProperty 
        name="ThresholdBetween" 
        command="ThresholdBetween"
        number_of_elements="2" />

     <IntVectorProperty 
        name="AllScalars" 
        command="SetAllScalars" 
        number_of_elements="1"
        default_values="1" > 
       <BooleanDomain/>
     </IntVectorProperty>
   <!-- End Threshold -->
   </SourceProxy>

   <SourceProxy name="ImageClip" class="vtkImageClip">
   <!-- TODO Extent entry -->
   <!-- End ImageClip -->
   </SourceProxy>

   <SourceProxy name="ShrinkFilter" class="vtkShrinkFilter">
      <DoubleVectorProperty
         name="ShrinkFactor"
         command="SetShrinkFactor"
         number_of_elements="1"
         default_values="0.5" >
        <DoubleRangeDomain min="0" max="1" />
      </DoubleVectorProperty>
   <!-- End ShrinkFilter -->
   </SourceProxy>

   <SourceProxy name="Clip" class="vtkPVClipDataSet">
     <ProxyProperty name="ClipFunction" command="SetClipFunction"/>
     <IntVectorProperty 
        name="InsideOut" 
        command="SetInsideOut" 
        number_of_elements="1"
        default_values="0" > 
       <BooleanDomain/>
     </IntVectorProperty>
   <!-- End Clip -->
   </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
