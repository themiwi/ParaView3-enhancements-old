# Optionally install the MySQL client libraries ...
IF(OVERVIEW_INSTALL_MYSQL)

  FIND_FILE(OVERVIEW_MYSQL_LIBRARY libmySQL.dll
    "c:/program files/MySQL/MySQL Server 5.0/bin"
    DOC "MySQL client interface library (libmySQL.dll)"
    )

  INSTALL(PROGRAMS
    ${OVERVIEW_MYSQL_LIBRARY}
    DESTINATION bin
    COMPONENT ${OVERVIEW_BRANDED_APPLICATION_TITLE}
    )

ENDIF(OVERVIEW_INSTALL_MYSQL)

# Optionally install the Oracle client libraries ...
